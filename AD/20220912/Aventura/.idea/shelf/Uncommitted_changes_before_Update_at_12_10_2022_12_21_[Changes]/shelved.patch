+import java.io.File;
+import java.util.ArrayList;
+
+/**
+ * Hello world!
+ *
+ */
+public class Main
+{
+    public static void main( String[] args )
+    {
+        ArrayList<EmpleadosJABX> listaFinal = new ArrayList<EmpleadosJABX>();
+        //Ruta del fichero en donde se va a leer el Xml
+        String ruta = "C:\\Users\\usuario\\Documents\\GitHub\\Daniel-Mallo-Lopez\\AD\\20220912\\JAX2\\src\\main\\java\\org\\example\\Empleados.xml";
+        try{
+            File file = new File(ruta);
+            if(file.exists()){
+                JAXBContext context = JAXBContext.newInstance(Empleado.class);
+                Unmarshaller unmarshaller = context.createUnmarshaller();
+
+                Empleado listaEmpleado = (Empleado) unmarshaller.unmarshal(file);
+                if(listaEmpleado != null){
+                    listaFinal = listaEmpleado.getListaEmpleados();
+                }
+                System.out.println(listaFinal);
+            }else{
+                System.out.println("No se ha encontrado el fichero");
+            }
+        }catch(Exception e){
+            e.printStackTrace();
+        }
+
+
+    }
