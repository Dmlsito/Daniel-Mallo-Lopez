+package org.example;
+
+import org.xml.sax.Attributes;
+import org.xml.sax.helpers.DefaultHandler;
+
+import java.util.ArrayList;
+import java.util.Locale;
+
+public class ParseadorEmpleadoSAX extends DefaultHandler {
+
+    private ArrayList<Empleado> listado;
+    private Empleado empleado;
+    private String valorElemento;
+
+    enum nodos{
+        EMPLEADOS,
+        EMPLEADO,
+        EDAD,
+        PUESTO,
+        HORARIO
+    }
+
+    public ParseadorEmpleadoSAX(){
+        super();
+    }
+    @Override
+    public void startDocument(){
+        listado = new ArrayList<Empleado>();
+    }
+
+    @Override
+public void startElement(String localName, String uri, String qName, Attributes attributes) {
+        if(localName != null){
+            switch(nodos.valueOf(localName.toUpperCase())){
+                case EMPLEADO:
+                    empleado = new Empleado();
+                    String nombre = attributes.getValue("Nombre");
+                    if(nombre != null){
+                        empleado.setNombre(nombre);
+                    }
+                    break;
+            }}}
+    @Override
+    public void endElement(String localName, String uri, String qName){
+        if(localName != null){
+            switch(nodos.valueOf(localName.toUpperCase())){
+                case EMPLEADO:
+                    listado.add(empleado);
+                    break;
+                case EDAD:
+                    empleado.setEdad(valorElemento);
+                    break;
+                case PUESTO:
+                    empleado.setPuesto(valorElemento);
+                    break;
+                case HORARIO:
+                    empleado.setHorario(valorElemento);
+                    break;
+            }}}
+
+
+    @Override
+    public void characters(char[] ch,  int start, int length){
+        valorElemento = new String (ch, start, length);
+    }
+
+    public ArrayList <Empleado> obtenerResultado(){
+        return listado;
+    }
+}
