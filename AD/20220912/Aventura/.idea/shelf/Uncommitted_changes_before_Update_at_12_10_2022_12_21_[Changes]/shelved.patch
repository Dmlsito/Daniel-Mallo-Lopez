+package org.example;
+
+import javax.xml.bind.JAXBContext;
+import javax.xml.bind.Unmarshaller;
+
+import java.io.File;
+import java.util.ArrayList;
+
+
+public class Main
+{
+    public static void main( String[] args )
+    {
+        try {
+            ejercicioJABX();
+        }catch(Exception e){
+            e.printStackTrace();
+        }
+
+    }
+
+           private static void ejercicioJABX(){
+            //Instanciamos un objeto de la clase fichero y le pasamos el path del archivo XML
+            File fichero = new File("C:\\Users\\usuario\\Documents\\GitHub\\Daniel-Mallo-Lopez\\AD\\20220912\\JAX1\\src\\main\\java\\org\\example\\mascotas.xml");
+            ArrayList <MascotaJABX> listaFinal = new ArrayList<MascotaJABX>();
+
+        try{
+
+            if(fichero.exists()){
+                JAXBContext context = JAXBContext.newInstance(Mascotas.class);
+                Unmarshaller unmarshaller = context.createUnmarshaller();
+
+                Mascotas listadoMascotas = (Mascotas) unmarshaller.unmarshal(fichero);
+
+                if(listadoMascotas != null){
+                    listaFinal = listadoMascotas.getListadoMascotas();
+                }
+
+                System.out.println(listaFinal);
+            }else{
+                System.out.println("Fichero no encontrado");
+            }
+        }catch(Exception e){
+            e.printStackTrace();
+        }
+    }
+}
